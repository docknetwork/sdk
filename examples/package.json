  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-sonarjs": "0.5.0",
    "eslint-plugin-unused-imports": "^3.0.0"
  }
