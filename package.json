{
  "name": "@docknetwork/sdk",
  "version": "0.5.3",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/docknetwork/sdk"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@substrate/txwrapper": "^7.0.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@digitalbazaar/ed25519-signature-2018": "^2.0.1",
    "@digitalbazaar/ed25519-verification-key-2018": "^3.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-wasm": "^5.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.5.0",
    "concurrently": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-sonarjs": "0.5.0",
    "eth-revert-reason": "^1.0.3",
    "ethers": "^5.0.32",
    "ethr-did-resolver": "^2.2.0",
    "glob": "^7.1.6",
    "jest": "24.5.0",
    "jsdoc": "^3.6.3",
    "rollup": "2.28.1",
    "typescript": "^4.2.3",
    "web3": "^1.3.4"
  },
  "scripts": {
    "exc": "npx babel-node",
    "dock-did-example": "npx babel-node example/dock-did.js",
    "did-resolver-example": "npx babel-node example/resolver.js",
    "revocation-example": "npx babel-node example/revocation.js",
    "vcdm-example": "npx babel-node example/vcdm.js",
    "standard-schemas-example": "npx babel-node example/standard_schemas.js",
    "schema-example": "npx babel-node example/schema.js",
    "schema-validation-example": "npx babel-node example/schema-validation.js",
    "blob-example": "npx babel-node example/blob.js",
    "claim-deduction-example": "npx babel-node example/claim-deduction.js",
    "anchor-example": "npx babel-node example/anchor.js",
    "test-examples": "yarn dock-did-example && yarn did-resolver-example && yarn revocation-example && yarn vcdm-example && yarn standard-schemas-example && yarn schema-example && yarn schema-validation-example && yarn blob-example && yarn claim-deduction-example && yarn anchor-example",
    "get-did": "npx babel-node scripts/get_did_doc.js",
    "upgrade_with_sudo": "npx babel-node scripts/runtime_upgrade_with_sudo.js",
    "onboard_with_sudo": "npx babel-node scripts/onboard_validator_with_sudo.js",
    "lint": "eslint \"src/**/*.js\"",
    "lint-examples": "eslint \"{src,example}/**/*.js\"",
    "lint-tests": "eslint \"{tests/unit,tests/integration}/**/*.js\"",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "testd": "jest --verbose ./tests/unit/issuing",
    "test": "jest --verbose ./tests/unit",
    "test-integration": "NODE_ENV=production jest --verbose --runInBand ./tests/integration",
    "test-poa": "NODE_ENV=production jest --verbose --runInBand ./tests/poa",
    "docs": "mkdir out && touch out/.nojekyll && jsdoc src -r -c .jsdoc -d out/reference",
    "dev-node": "./scripts/run_node_in_docker --dev --rpc-external --ws-external --rpc-cors=all",
    "test-with-node": "./scripts/with_docker_test_node yarn test-integration",
    "test-poa-with-node": "./scripts/with_docker_test_node yarn test-poa",
    "examples-with-node": "./scripts/with_docker_test_node yarn test-examples",
    "mdbook": "mdbook build tutorials --dest-dir ../out/tutorials",
    "type-check": "tsc --allowJs --checkJs --noEmit --moduleResolution node --resolveJsonModule --target ES6 --skipLibCheck true --allowSyntheticDefaultImports true"
  },
  "dependencies": {
    "@comunica/actor-init-sparql-rdfjs": "^1.19.2",
    "@docknetwork/node-types": "^0.4.7",
    "@polkadot/api": "^5.3.2",
    "@polkadot/keyring": "^7.1.1",
    "@polkadot/types": "^5.3.2",
    "@polkadot/types-known": "5.3.2",
    "@polkadot/util": "^7.1.1",
    "@polkadot/util-crypto": "^7.1.1",
    "@types/jest": "^26.0.24",
    "axios": "0.21.1",
    "babel-polyfill": "^6.26.0",
    "base58-universal": "^1.0.0",
    "base64url": "^3.0.1",
    "blake2b": "^2.1.3",
    "bs58": "^4.0.1",
    "deep-equal": "^2.0.3",
    "did-resolver": "^1.1.0",
    "dotenv": "^8.2.0",
    "elliptic": "^6.5.4",
    "ipfs-http-client": "^49.0.4",
    "js-sha256": "^0.9.0",
    "json-canonicalize": "^1.0.3",
    "jsonld": "^5.2.0",
    "jsonld-signatures": "^9.0.2",
    "jsonschema": "^1.2.6",
    "mrklt": "^0.2.0",
    "n3": "^1.8.1",
    "rify": "^0.7.0"
  }
}
