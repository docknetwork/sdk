  "jest": {
    "testEnvironment": "node"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "ecdsa-secp256k1-signature-2019": "^1.0.0",
    "@polkadot/api": "^1.8.1",
    "vc-js": "^0.5.0",
    "did-resolver": "^1.1.0",
    "elliptic": "^6.5.2",
